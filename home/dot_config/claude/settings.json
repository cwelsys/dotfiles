      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(cp:*)",
      "Bash(rg:*)",
      "Bash(fd:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(bat:*)",
      "Bash(tre:*)",
      "Bash(wget:*)",
      "Bash(curl:*)",
      "Bash(delta:*)",
      "Bash(git add:*)",
      "Bash(git reset:*)",
      "Bash(find:*)",
      "Bash(echo:*)",
      "Bash(grep:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr diff:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git stash:*)",
      "Bash(git fetch:*)",
      "Bash(git diff:*)",
