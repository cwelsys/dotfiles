$sudo$username$shell$character"""

[os]
format = '[$symbol ](lavender)'
disabled = false

[character]
success_symbol = "[❯](peach)"
error_symbol = "[](mauve bold)"
vimcmd_symbol = "[❮](subtext1)"

[username]
style_user = 'yellow'
style_root = 'red'
format = '[$user]($style)'
show_always = false
aliases = { "Administrator" = '󱐋 ' }

[shell]
fish_indicator = '󰈺 '
bash_indicator = ' '
nu_indicator = '󰟆 '
unknown_indicator = ' '
style = 'teal bold'
disabled = false
format = '[$indicator]($style)'
{{ if eq .chezmoi.os "windows" }}
powershell_indicator = ''
{{ else }}
zsh_indicator = ''
powershell_indicator = '_'
{{ end }}

[battery]
full_symbol = "󰁹 "
charging_symbol = "󰂄 "
discharging_symbol = "󰂃 "
unknown_symbol = "󰁽 "
empty_symbol = "󰂎 "
disabled = false
format = "[$symbol$percentage]($style) "

[cmd_duration]
min_time = 2000
format = "[$duration]($style) "
style = "dimmed sapphire"
show_milliseconds = false
disabled = false
show_notifications = false
min_time_to_notify = 45000

[[battery.display]]
threshold = 10
style = "red bold"

[vagrant]
format = " [$symbol($version )]($style)"
symbol = "⋁"
style = "sky bold"
disabled = false

[directory]
truncation_length = 2
fish_style_pwd_dir_length = 2
read_only_style = '197'
read_only = " 󰌾"
style = "sapphire"
format = '[$path]($style)[$read_only]($read_only_style) '

[hostname]
ssh_symbol = " "
disabled = true

[git_branch]
style = "subtext1"
truncation_length = 25
format = '[ ](peach)[$branch]($style) '

[git_commit]
tag_symbol = '  '
commit_hash_length = 4

[git_status]
format = '[$all_status$ahead_behind]($style) '
style = 'bold green'
conflicted = '🏳'
up_to_date = ''
untracked = ' '
ahead = '⇡${count}'
diverged = '⇕⇡${ahead_count}⇣${behind_count}'
behind = '⇣${count}'
stashed = ' '
modified = ' '
staged = '[++\($count\)](green)'
renamed = '襁 '
deleted = ' '

[memory_usage]
symbol = "󰍛 "

[docker_context]
symbol = " "

[aws]
symbol = "  "
disabled = true

