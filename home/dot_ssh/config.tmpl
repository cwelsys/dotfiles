# vim:ft=sshconfig.chezmoitmpl
Include ~/.ssh/1Password/config
Host *
ServerAliveInterval 86400
ServerAliveCountMax 4
Port 22
IdentityFile ~/.ssh/cwel

Host pbox
HostName pbox
user cwel
ForwardAgent yes

Host github.com
User git
PreferredAuthentications publickey

Host git.cwel.sh
HostName git.cwel.sh
User git
PreferredAuthentications publickey
{{ end }}
