# vim:ft=sshconfig.chezmoitmpl
Include ~/.ssh/1Password/config
Host *
ServerAliveInterval 86400
ServerAliveCountMax 4
Port 22
IdentityFile ~/.ssh/cwel

Host pbox
HostName pbox
user cwel
ForwardAgent yes
ForwardX11 yes

Host pstick
HostName pstick
User cwel
ForwardAgent yes
ForwardX11 yes

Host github.com
User git
PreferredAuthentications publickey

Host gitea
HostName git.cwel.casa
User git
PreferredAuthentications publickey
